<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="cn.yuan.uitest">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".login.SetLoginPasswordActivity">
        </activity>
        <activity android:name=".login.VerificationCodeActivity">
        </activity>
        <activity android:name=".login.LoginPhoneActivity">
        </activity>
        <activity android:name=".login.ForgetPasswordActivity">
        </activity>
        <activity android:name=".login.RegisterActivity">
        </activity>
        <activity
                android:name=".login.LoginActivity"
                android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>
    </application>

</manifest>